<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mousty.condorcet.be.gestion_score">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="be.mousty.intent.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="be.mousty.intent.HomeActivity"></activity>
        <activity android:name="be.mousty.intent.AddScoreActivity"></activity>
        <activity android:name="be.mousty.intent.DisplayTopTenActivity"></activity>
        <activity android:name="be.mousty.intent.DisplayGameListActivity"></activity>
    </application>

</manifest>